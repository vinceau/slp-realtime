{
  "name": "@vinceau/slp-realtime",
  "description": "Realtime slp parsing",
  "license": "MIT",
  "version": "3.1.0",
  "repository": "vinceau/slp-realtime",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "coverage": "yarn run test --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublishOnly": "yarn run lint && yarn run test && yarn run build",
    "docs": "yarn typedoc --excludePrivate --excludeNotExported --exclude \"**/*+(.spec).ts\"",
    "postdocs": "touch docs/.nojekyll"
  },
  "keywords": [
    "slp",
    "slippi",
    "realtime"
  ],
  "dependencies": {
    "@slippi/slippi-js": "^5.1.1",
    "chokidar": "^3.3.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "tailstream": "^0.1.0"
  },
  "peerDependencies": {
    "rxjs": "^6.x"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.3",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.2.1",
    "open-cli": "^6.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "rxjs": "^6.5.4",
    "sinon": "^7.5.0",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
